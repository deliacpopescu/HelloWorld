@model TeamInfo
@{
    ViewData["Title"] = "Home Page";
}
@section scripts {
    <script type="text/javascript" src="~/js/Index.js"></script>
}

<div class="text-center">
    <h1 class="display-4">Our team -Team 2</h1>
    <ul id="list">
        @for (var i = 0; i < Model.TeamMembers.Count; i++)
        {
            var member = Model.TeamMembers[i];

            <li class="member">
                 @member
             </li>
         }
    </ul>
    <input id="nameField" type="text" title="Name" />
    <button type="submit" class ="btn-danger" id="createButton">Add</button>

</div>
